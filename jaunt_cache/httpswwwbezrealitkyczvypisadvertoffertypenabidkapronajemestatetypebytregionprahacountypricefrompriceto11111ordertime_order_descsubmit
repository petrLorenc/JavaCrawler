                    var countReportError = 0;
                            function reportError(message, file, lineNumber) {
                                var url = "//www.bezrealitky.cz/api/utils/js-log";
                                var values = {"message": message, "file": file, "lineNumber": lineNumber, "location": window.location.href};

                                var forms = {};

                                var inputs = $("input, select, textarea");

                                for (var index = 0, length = inputs.length; index < length; index++) {
                                    var element = $(inputs[index]);
                                    forms[element.attr("name")] = element.val();

                                    if (element.attr("type") === "checkbox") {
                                        forms[element.attr("name")] = element.prop("checked");
                                    }
                                }

                                values.forms = forms;

                                values.browser = navigator.userAgent;
                                if (countReportError < 10) {
//                            $.post(url, values);
                                }
                                countReportError++;
                            };

                    window.onerror = function (message, file, lineNumber) {
                        reportError(message, file, lineNumber);
                    };
        </script>
